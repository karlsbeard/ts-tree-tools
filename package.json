{
  "name": "ts-tree-tools",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@9.5.0",
  "description": "the tools for tree",
  "author": "Karl <2557075675@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlsbeard/ts-tree-tools.git"
  },
  "keywords": [
    "tree",
    "tools",
    "list"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "release": "release-it"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/node": "^20.14.12",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "9.5.0",
    "lint-staged": "^15.2.7",
    "release-it": "^17.6.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^5.3.4",
    "vitest": "^2.0.4",
    "vue-tsc": "^2.0.24"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
